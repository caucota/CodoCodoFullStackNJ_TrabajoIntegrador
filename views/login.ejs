<!DOCTYPE html>
<html lang="en">
  <%-include("partials/head")%>
  <body>
    <%-include("partials/header")%>
    <main id="main__login">
      <h2 class="titulo__ingresar">INGRESAR A MI CUENTA</h2>
      <form class="form__login" action="" method="post">
          <div class="div__input">
              <label class="label__login" for="email">Email:</label>
              <input class="input__login" type="text" name="email" id="email" placeholder="johndoe@email.com">
          </div>
          <div class="div__input">
              <label class="label__login" for="password">Contraseña:</label>
              <input class="input__login" type="password" name="password" id="password" placeholder="&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;">
          </div>
          <div class="div__submit">
              <input class="button__input" type="submit" value="INGRESAR">
              <div class="check__recordar">
                  <input type="checkbox" name="recodar" id="">
                  <label for="">Recordarme</label>
              </div>
          </div>
          <a class="link" href="#">Olvidé mi contraseña</a>
      </form>
    </main>

    <%-include("partials/footer")%>
  </body>
</html>
